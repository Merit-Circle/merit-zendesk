{{#is settings.font_family_base "'Helvetica Neue', Arial, Helvetica, sans-serif"}}
  <style type="text/css">
    @font-face {
      font-family: "Helvetica Neue";
      src: url("{{asset 'HelveticaNeue-Thin.woff2'}}") format("woff2"),
            url("{{asset 'HelveticaNeue-Thin.woff'}}") format('woff');
      font-weight: 200;
      font-style: normal;
    }
    @font-face {
      font-family: "Helvetica Neue";
      src: url("{{asset 'HelveticaNeue-Light.woff2'}}") format("woff2"),
            url("{{asset 'HelveticaNeue-Light.woff'}}") format('woff');
      font-weight: 300;
      font-style: normal;
    }
    @font-face {
      font-family: "Helvetica Neue";
      src: url("{{asset 'HelveticaNeue.woff2'}}") format("woff2"),
            url("{{asset 'HelveticaNeue.woff'}}") format('woff');
      font-weight: 400;
      font-style: normal;
    }
  </style>
{{/is}}

{{#is settings.font_family_base "'Metaverse', sans-serif"}}
  <style type="text/css">
    @font-face {
      font-family: "Metaverse";
      src: url("{{asset 'Metaverse-Regular.woff2'}}") format("woff2"),
